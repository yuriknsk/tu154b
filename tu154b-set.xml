    <key n="115">
	    <name>s</name>
	    <desc>Auto Startup</desc>
	    <binding>
		    <command>nasal</command>
		    <script>controls.autostart()</script>
	    </binding>
	    <mod-up>
		    <binding>
			    <command>nasal</command>
			    <script>print("Autostart")</script>
		    </binding>		    
	    </mod-up>
    </key>

    <key n="100">
      <name>d</name>
      <desc>Autopilot on</desc>
      <binding>
        <command>nasal</command>
        <script>
	  setprop("tu154/switches/pu-46-kren", 1.0);
	  setprop("tu154/switches/pu-46-tang", 1.0);
	  absu.absu_stab_on();
	</script>
      </binding>
    </key>
    
    <key n="68">
      <name>D</name>
      <desc>Autothrottle on</desc>
      <binding>
        <command>nasal</command>
        <script>
	  absu.absu_at_start();
	</script>
      </binding>
    </key>

    <key n="4">
      <name>ctrl+D</name>
      <desc>All AP off</desc>
      <binding>
        <command>nasal</command>
        <script>
	  absu.absu_stab_off();
	  absu.absu_at_stop();
	</script>
      </binding>
    </key>

<!-- Nominal -->
    <key n="78">
      <name>N</name>
      <desc>Nominal thrust</desc>
