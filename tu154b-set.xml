<PropertyList>

 <sim>
	 <description>Tupolev Tu-154b airliner</description>
	 <author>Yurik V. Nikiforoff aka Yurik_nsk</author>

	 
	 <status>advanced production</status>
	 <rating>
	  <FDM type="int">5</FDM>
	  <systems type="int">5</systems>
	  <cockpit type="int">5</cockpit>
	  <model type="int">5</model>
	</rating>
	
	 <help>
	   <title>Tu-154B-2</title>
   	   <key>
	    <name>s</name>
	    <desc>Autostart</desc>
	   </key>
	   <key>
	    <name>d/D/Ctrl+D</name>
	    <desc>AP On/Autothrottle On/AP-AT Off </desc>
	   </key>
	   <key>
	    <name>N/n/Ctrl+N/*</name>
	    <desc>Set nominal/0.85/0.7/0.0 thrust</desc>
	   </key>
	   <key>
	    <name>j/k</name>
	    <desc>Spoilers control</desc>
	   </key>
	   <key>
	    <name>F2</name>
	    <desc>Thrust reverser On</desc>
	   </key>
	   <key>
	    <name>Delete</name>
	    <desc>Center aileron, elevator, and rudder</desc>
	   </key>
	 </help>

<menubar>
  <default>
    <menu n="10">
      <label>Tu-154B-2</label>
      <enabled type="bool">true</enabled>
